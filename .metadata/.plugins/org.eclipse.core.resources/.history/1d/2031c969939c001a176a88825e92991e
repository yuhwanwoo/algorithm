package programmers_summercoding;

public class exam01 {

	public static void main(String[] args) {
		
		solution(1987);

		System.out.println(solution(1987));
	}

	public static int solution(int p) {
		int answer=0;
		String[] spl=null;
		
		for(int i=p+1;i<10000;i++) {
			spl=(i+"").split("");
		}
		
		for(int i=0;i<spl.length;i++) {
			
		}
		
		return answer;
	}
}
