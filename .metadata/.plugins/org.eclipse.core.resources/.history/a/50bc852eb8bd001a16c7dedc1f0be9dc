package booCam;

import java.util.HashMap;

public class Exam01 {

	public static void main(String[] args) {
		String[] name_list = { "º½", "¿©¿ï", "º½º½" };

		solution(name_list);
	}

	public static boolean solution(String[] name_list) {
		boolean answer = false;
		HashMap<String, Integer> map = new HashMap<>();

		for (int i = 0; i < name_list.length; i++) {
			map.put(name_list[i], i);
		}

		for (int i = 0; i < name_list.length; i++) {
			for (int k = 0; k < name_list.length; k++) {
				System.out.println(map.get(name_list[i]));
				System.out.println(i);
				if (map.get(name_list[i]) != k) {
					String temp = "";
					String[] spl = name_list[i].split("");
					for (int j = 0; j < spl.length; j++) {
						temp = temp + spl[j];
						System.out.println(temp);
						if (map.containsKey(temp)) {
							answer = true;
						}
					}
				}
			}
		}
		System.out.println(answer);

		return answer;
	}

}
