package kakao2020intern;

import java.util.Queue;

public class test01 {

	public static void main(String[] args) {
		int[] numbers={1, 3, 4, 5, 8, 2, 1, 4, 5, 9, 5};
		String hand="right";
		
		solution(numbers, hand);
	}

	public static String solution(int[] numbers,String hand) {
		
		
		String answer="";
		lefthand lh=new lefthand("*");
		righthand rh=new righthand("#");
		for(int i=0;i<numbers.length;i++) {
			if(numbers[i]==1|numbers[i]==4|numbers[i]==7) {
				lh.x=numbers[i]+"";
				answer=answer+"L";
				System.out.println("lh.x의 값은"+lh.x);
			}else if(numbers[i]==3|numbers[i]==6|numbers[i]==9) {
				rh.x=numbers[i]+"";
				answer=answer+"R";
				System.out.println("rh.x의 값은"+rh.x);
			}else if(hand.equals("left")&&numbers[i]==2&&(rh.x.equals("3")&&lh.x.equals("1"))||(rh.x.equals("6")&&lh.x.equals("4"))||(rh.x.equals("9")&&lh.x.equals("7"))) {
				lh.x=numbers[i]+"";
				answer=answer+"L";
			}else if(hand.equals("left")&&numbers[i]==5&&(rh.x.equals("3")&&lh.x.equals("1"))||(rh.x.equals("6")&&lh.x.equals("4"))||(rh.x.equals("9")&&lh.x.equals("7"))) {
				lh.x=numbers[i]+"";
				answer=answer+"L";
			}else if(hand.equals("left")&&numbers[i]==8&&(rh.x.equals("3")&&lh.x.equals("1"))||(rh.x.equals("6")&&lh.x.equals("4"))||(rh.x.equals("9")&&lh.x.equals("7"))) {
				lh.x=numbers[i]+"";
				answer=answer+"L";
			}else if(hand.equals("left")&&numbers[i]==0&&(rh.x.equals("3")&&lh.x.equals("1"))||(rh.x.equals("6")&&lh.x.equals("4"))||(rh.x.equals("9")&&lh.x.equals("7"))) {
				lh.x=numbers[i]+"";
				answer=answer+"L";
			}else if(hand.equals("right")&&numbers[i]==2&&(rh.x.equals("3")&&lh.x.equals("1"))||(rh.x.equals("6")&&lh.x.equals("4"))||(rh.x.equals("9")&&lh.x.equals("7"))) {
				rh.x=numbers[i]+"";
				answer=answer+"R";
			}else if(hand.equals("right")&&numbers[i]==5&&(rh.x.equals("3")&&lh.x.equals("1"))||(rh.x.equals("6")&&lh.x.equals("4"))||(rh.x.equals("9")&&lh.x.equals("7"))) {
				rh.x=numbers[i]+"";
				answer=answer+"R";
			}else if(hand.equals("right")&&numbers[i]==8&&(rh.x.equals("3")&&lh.x.equals("1"))||(rh.x.equals("6")&&lh.x.equals("4"))||(rh.x.equals("9")&&lh.x.equals("7"))) {
				rh.x=numbers[i]+"";
				answer=answer+"R";
			}else if(hand.equals("right")&&numbers[i]==0&&(rh.x.equals("3")&&lh.x.equals("1"))||(rh.x.equals("6")&&lh.x.equals("4"))||(rh.x.equals("9")&&lh.x.equals("7"))) {
				rh.x=numbers[i]+"";
				answer=answer+"R";
			}else if(numbers[i]==2&&lh.x.equals("1")&&((rh.x.equals("6"))||(rh.x.equals("9"))||(rh.x.equals("#")))) {
				lh.x=numbers[i]+"";
				answer=answer+"L";
			}else if(numbers[i]==2&&rh.x.equals("3")&&((lh.x.equals("4"))||(lh.x.equals("7"))||(lh.x.equals("*")))) {
				rh.x=numbers[i]+"";
				answer=answer+"R";
			}else if(numbers[i]==5&&lh.x.equals("1")&&((rh.x.equals("6"))||(rh.x.equals("9"))||(rh.x.equals("#")))) {
				lh.x=numbers[i]+"";
				answer=answer+"L";
			}else if(numbers[i]==5&&rh.x.equals("3")&&((lh.x.equals("4"))||(lh.x.equals("7"))||(lh.x.equals("*")))) {
				rh.x=numbers[i]+"";
				answer=answer+"R";
			}
		}
		
		return answer;
	}
	public static int pandan(int stand,String lh,String rh) {
		if(stand==2) {
			if(lh.equals("1")||rh.equals("3")) {
				return 1;
			}else if(lh.equals("4")||rh.equals("6")) {
				return 2;
			}else if(lh.equals("7")||rh.equals("9")) {
				return 3;
			}else if(lh.equals("*")||rh.equals("#")) {
				return 4;
			}
		}
		return 0;
	}
}

class lefthand{
	String x;
	lefthand(String x){
		this.x=x;
	}
}
class righthand{
	String x;
	righthand(String x){
		this.x=x;
	}
}