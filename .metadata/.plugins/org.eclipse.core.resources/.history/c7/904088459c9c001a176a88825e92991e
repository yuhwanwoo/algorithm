package programmers_summercoding;

import java.util.ArrayList;

public class exam02 {

	public static void main(String[] args) {
		System.out.println(solution(0));
	}
	
	public static long solution(long n) {
        long answer = 0;
        ArrayList<Integer> list_arr=new ArrayList<>();
        
        for(int i=0;i<=10;i++) {
        	list_arr.add((int) Math.pow(3, i));
        }
        
        for(int i=0;i<list_arr.size();i++) {
        	System.out.println(list_arr.get(i));
        }
        
        ArrayList<Integer> ans_list=new ArrayList<>();
        for(int i=1;i<11;i++) {
        	for(int j=i+1;j<11;j++) {
        		ans_list.get(list_arr.get(i)+list_arr.get(j));
        	}
        }
        
        for(int i=0;i<ans_list.size();i++) {
        	System.out.println(ans_list.get(i));
        }
        
        
        return answer;
	}

}
