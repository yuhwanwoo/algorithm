package kakao2020intern;

public class test04 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

	public static int solution(int[][] board) {
		int answer=0;
		
		Car car1=new Car(0, 0);
		Car car2=new Car(0, 0);
		
		soon(car1,board);
		soon(car2,board);
		
		if(soon(car1,board)>soon(car2,board)) {
			answer=soon(car2,board);
		}else {
			answer=soon(car1,board);
		}
        return answer;
	}
	
	public static int soon(Car car,int[][] board) {
		if(car.z==1) {
			car.x
		}else if(car.z==2) {
			
		}
		return 0;
	}
}

class Car{
	int x;
	int y;
	
	public Car(int x, int y) {
		super();
		this.x = x;
		this.y = y;
		
	}
	
}
