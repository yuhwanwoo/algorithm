		String hand="right";
		
		solution(numbers, hand);
	}

	public static String solution(int[] numbers,String hand) {
		
		
		String answer="";
		lefthand lh=new lefthand();
		for(int i=0;i<numbers.length;i++) {
			
			if(numbers[i]==1|numbers[i]==4|numbers[i]==7) {
				answer=answer+"L";
			}else if(numbers[i]==3|numbers[i]==6|numbers[i]==9) {
				answer=answer+"R";
			}else if(hand.equals("right")&&(numbers[i]==2|numbers[i]==5|numbers[i]==8|numbers[i]==0)) {
				
			}
		}
		
		return answer;
	}
}

class lefthand{
	String x;
	lefthand(String x){
		this.x=x;
	}
}
class righthand{
	int x;
