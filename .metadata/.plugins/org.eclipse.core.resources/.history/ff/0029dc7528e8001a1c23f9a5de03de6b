package programmers_lev02;


// https://programmers.co.kr/learn/courses/30/lessons/12980
// 프로그래머스 12980 점프와순간이동

public class JumpAndTeleport {

	public static void main(String[] args) {
		int n=5;
		
		solution(n);
	}

	public static int solution(int n) {
        int ans = 0;
        
        while(n != 0){
            if(n % 2 == 0){
                n /= 2;
            } else {
                n--;
                ans++;
            }
        }

        return ans;
    }
}
