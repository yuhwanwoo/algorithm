package kakao2020intern;

import java.util.Queue;

public class test01 {

	public static void main(String[] args) {
		int[] numbers={1, 3, 4, 5, 8, 2, 1, 4, 5, 9, 5};
		String hand="right";
		
		solution(numbers, hand);
	}

	public static String solution(int[] numbers,String hand) {
		
		
		String answer="";
		lefthand lh=new lefthand("*");
		righthand rh=new righthand("#");
		for(int i=0;i<numbers.length;i++) {
			if(numbers[i]==1|numbers[i]==4|numbers[i]==7) {
				lh.x=numbers[i]+"";
				answer=answer+"L";
				System.out.println("lh.x의 값은"+lh.x);
			}else if(numbers[i]==3|numbers[i]==6|numbers[i]==9) {
				rh.x=numbers[i]+"";
				answer=answer+"R";
				System.out.println("rh.x의 값은"+rh.x);
			}else if(hand.equals("left")&&numbers[i]==2&&rh.x.equals("1")&&lh.x.equals("3")) {
				answer=answer+"L";
			}
		}
		
		return answer;
	}
}

class lefthand{
	String x;
	lefthand(String x){
		this.x=x;
	}
}
class righthand{
	String x;
	righthand(String x){
		this.x=x;
	}
}