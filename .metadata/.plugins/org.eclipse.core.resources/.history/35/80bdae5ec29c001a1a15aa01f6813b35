package midas_codingTest;

public class Exam02 {

	public static void main(String[] args) {
		
		int[][] target = {{0,1},{-1,1},{1,0},{-2,2}};
		solution(-1, 2, 2, 60, target);

	}
	
	public static int solution(int x,int y,int r, int d, int[][] target) {
		int answer=-1;
		double tan=Math.toDegrees(Math.atan((double)y/x));
		tan=tan+180;
		
		System.out.println(tan);
		double fir=tan-d;
		double fin=tan+d;
		
		System.out.println(fir);
		System.out.println(fin);
		for(int i=0;i<target.length;i++) {
			int tx=target[i][0];
			int ty=target[i][1];
			if(Math.pow(tx, 2)+Math.pow(tx, 2)<Math.pow(r, 2)&&
					Math.toDegrees(Math.atan((double)ty/tx))>fir
					&&Math.toDegrees(Math.atan((double)ty/tx))<fin) {
				
			}
			
		}
		
		return answer;
	}

}
