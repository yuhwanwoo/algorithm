package kakao2020intern;

import java.util.Queue;

public class test01 {

	public static void main(String[] args) {
		int[] numbers={1, 3, 4, 5, 8, 2, 1, 4, 5, 9, 5};
		String hand="right";
		
		solution(numbers, hand);
	}

	public static String solution(int[] numbers,String hand) {
		
		
		String answer="";
		lefthand lh=new lefthand("*");
		righthand rh=new righthand("#");
		for(int i=0;i<numbers.length;i++) {
			if(numbers[i]==1|numbers[i]==4|numbers[i]==7) {
				lh.x=numbers[i]+"";
				answer=answer+"L";
			}else if(numbers[i]==3|numbers[i]==6|numbers[i]==9) {
				rh.x=numbers[i]+"";
				answer=answer+"R";
			}else if(numbers[i]==2) {
				pandan(numbers[i],lh.x,rh.x);
			}
		}
		
		return answer;
	}
	public static int pandan(int stand,String lh,String rh) {
		if(stand==2) {
			if(lh.equals("1")||rh.equals("3")) {
				return 1;
			}else if(lh.equals("4")||rh.equals("6")) {
				return 2;
			}else if(lh.equals("7")||rh.equals("9")) {
				return 3;
			}else if(lh.equals("*")||rh.equals("#")) {
				return 4;
			}
		}else if(stand==5) {
			if(lh.equals("1")||rh.equals("3")) {
				return 2;
			}else if(lh.equals("4")||rh.equals("6")) {
				return 1;
			}else if(lh.equals("7")||rh.equals("9")) {
				return 2;
			}else if(lh.equals("*")||rh.equals("#")) {
				return 3;
			}
		}else if(stand==8) {
			if(lh.equals("1")||rh.equals("3")) {
				return 3;
			}else if(lh.equals("4")||rh.equals("6")) {
				return 2;
			}else if(lh.equals("7")||rh.equals("9")) {
				return 1;
			}else if(lh.equals("*")||rh.equals("#")) {
				return 2;
			}
		}else if(stand==0) {
			if(lh.equals("1")||rh.equals("3")) {
				return 4;
			}else if(lh.equals("4")||rh.equals("6")) {
				return 3;
			}else if(lh.equals("7")||rh.equals("9")) {
				return 2;
			}else if(lh.equals("*")||rh.equals("#")) {
				return 1;
			}
		}
				
		return 0;
	}
}

class lefthand{
	String x;
	lefthand(String x){
		this.x=x;
	}
}
class righthand{
	String x;
	righthand(String x){
		this.x=x;
	}
}